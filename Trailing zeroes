
#include<bits/stdc++.h>
using namespace std;
#define lli long long int
#define ulli unsigned long long int
#define MOD 1000000007
int solve(int n){
    int ans=0;
    int D=5;
    while(n/D > 0){
        ans += n/D;
        D = D*5;
    }
    return ans;
}

int main()
{

            int n;
            cin>>n;
            cout<<solve(n)<<endl;





}


